{
  "name": "ng-alt-sidebar-example",
  "version": "17.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve ng-alt-sidebar-example",
    "lint": "npm run example:lint && npm run lib:lint",
    "test": "npm run example:test && npm run lib:test",
    "test:ci": "npm run example:test:ci && npm run lib:test:ci",
    "test:lib-coverage": "npm run example:test:lib-coverage && npm run lib:test:lib-coverage",
    "example:build": "ng build ng-alt-sidebar-example",
    "example:build:prod": "ng build ng-alt-sidebar-example --base-href=/ng-alt-sidebar/ --output-path=demo/",
    "example:lint": "ng lint ng-alt-sidebar-example",
    "example:test": "ng test ng-alt-sidebar-example --browsers=Chrome --watch false --reporters junit,progress,kjhtml",
    "example:test:ci": "ng test ng-alt-sidebar-example -c=ci --browsers=ChromeHeadlessCI --watch=false --progress=false --reporters junit,progress,kjhtml",
    "example:test:lib-coverage": "ng test ng-alt-sidebar-example --code-coverage --watch=false",
    "lib:build": "ng build ng-alt-sidebar && cd dist/ng-alt-sidebar && npm pack",
    "lib:build:prod": "ng build ng-alt-sidebar && cd dist/ng-alt-sidebar && npm pack",
    "lib:build:prod:publish": "ng build ng-alt-sidebar && cd dist/ng-alt-sidebar && npm pack && npm publish",
    "lib:lint": "ng lint ng-alt-sidebar",
    "lib:test": "ng test ng-alt-sidebar --browsers=Chrome --watch false --reporters junit,progress,kjhtml",
    "lib:test:ci": "ng test ng-alt-sidebar -c=ci --browsers=ChromeHeadlessCI --watch=false --progress=false --reporters junit,progress,kjhtml",
    "lib:test:lib-coverage": "ng test ng-alt-sidebar --code-coverage --watch=false",
    "prettier:check": "prettier --check !.angular/** ./**/*.{ts,js,scss,html,json}",
    "prettier:write": "prettier --write !.angular/** ./**/*.{ts,js,scss,html,json}",
    "prepare": "npm run lib:build:prod && husky install",
    "postinstall": "webdriver-manager update --standalone false --gecko false"
  },
